    "# Arithmetic Operators in Python\n",
    "x = 10\n",
    "y = 3\n",
    "print(x + y)  # Addition 13\n",
    "print(x - y)  # Subtraction 7\n",
    "print(x * y)  # Multiplication 30\n",
    "print(x / y)  # Division 3.3333333333333335\n",
    "print(x // y) # Floor Division 3\n",
    "print(x % y)  # Modulus 1\n",
    "print(x ** y) # Exponentiation 1000"
   ]
